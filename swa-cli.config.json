{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "dw-swa-demo": {
      "appLocation": ".",
      "apiLocation": "api/src/api",
      "outputLocation": "dist",
      "apiLanguage": "dotnetisolated",
      "apiVersion": "7.0",
      "appBuildCommand": "yarn run build",
      "apiBuildCommand": "dotnet publish -c Release",
      "run": "yarn run start",
      "appDevserverUrl": "http://localhost:4200",
      "port": 4000,
      "appName": "swa-dw-demo",
      "resourceGroup": "dw-swa-demo",
      "apiPort": 7101,
      "open": true
    }
  }
}
